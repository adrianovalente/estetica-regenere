<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adriano.esteticaregenere_android" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SET_DEBUG_APP"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:noHistory="true"
            android:name=".Activities.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".Activities.BaseMenuActivity"
            android:label="POC Menu">
        </activity>

        <activity
            android:name=".Activities.HomeActicity"
            android:label="Home Activity">
        </activity>

        <activity
            android:name=".Activities.ScheduleAppointmentActivity"
            android:label="Marcar consulta">
        </activity>
    </application>

</manifest>
